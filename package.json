{
  "name": "raz-ui",
  "version": "0.0.37",
  "description": "raz react components",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "*.md"
  ],
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "clean": "rm -rf lib",
    "prebuild": "npm run clean && tsc",
    "build": "babel src --extensions \".ts,.tsx\" -d lib",
    "postbuild": "./finalize-build.sh",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint --ext .tsx --ignore-path .gitignore .",
    "cov": "cov",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "package": "npm run test && npm run build && npm publish --public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aref00/raz-ui.git"
  },
  "keywords": [
    "raz",
    "ui",
    "react",
    "component"
  ],
  "author": "Aref Akbari <aref@pchas.ir> (http://pchas.ir/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aref00/raz-ui/issues"
  },
  "homepage": "https://github.com/aref00/raz-ui#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-cli": "^6.26.0",
    "babel-jest": "^28.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-scss": "^1.1.0",
    "babel-preset-minify": "^0.5.2",
    "cov": "^0.1.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^29.0.2",
    "minify": "^9.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.8.2",
    "uglify-js": "^3.17.0"
  },
  "dependencies": {
    "apexcharts": "^3.35.5",
    "react-apexcharts": "^1.4.0"
  }
}
